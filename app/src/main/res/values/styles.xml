<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#27AE60</item>
        <item name="colorPrimaryDark">#27AE60</item>
        <item name="colorAccent">#FFFFFF</item>
        <item name="android:navigationBarColor">#27AE60</item>
    </style>
    <style name="AlertDialogCustom" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:dialogMessage">@android:color/white</item>
        <item name="android:headerDividersEnabled">true</item>
        <item name="android:background">#27AE60</item>
    </style>
    <style name="AppTheme_2" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="android:windowTranslucentNavigation">true</item>

    </style>
</resources>